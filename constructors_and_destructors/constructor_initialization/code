#include <iostream>
#include <string>
#include <cmath>

class Location {
public:
    Location(const std::string& name, double x, double y) {
        this->name = name;
        this->x = x;
        this->y = y;
    }

    double distanceTo(const Location& other) {
        double dx = other.x - this->x;
        double dy = other.y - this->y;
        return std::sqrt(dx * dx + dy * dy);
    }

    std::string name;
    double x;
    double y;
};

int main() {
    Location home("Home", 0.0, 0.0);
    Location school("Office", 3.0, 4.0);

    std::cout << "Distance from " << home.name << " to " << school.name 
              << ": " << home.distanceTo(school) << std::endl;

    return 0;
}